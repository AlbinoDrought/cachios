{
  "name": "cachios",
  "private": true,
  "version": "1.0.6",
  "description": "Simple axios cache wrapper using node-cache",
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "scripts": {
	"lint": "eslint src",
    "build": "node build/build.js",
    "test": "cross-env BABEL_ENV=test karma start ./tests/karma.conf.js --single-run"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbinoDrought/cachios.git"
  },
  "keywords": [
    "axios",
    "cache"
  ],
  "author": "AlbinoDrought",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbinoDrought/cachios/issues"
  },
  "homepage": "https://github.com/AlbinoDrought/cachios#readme",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-runtime": "^6.23.0",
    "node-cache": "^4.1.1",
    "object-hash": "^1.1.8"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.1.0",
    "cross-env": "^5.0.0",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.3.0",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "mocha": "^4.0.1",
    "moxios": "^0.4.0",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "sinon": "^4.0.1",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  }
}
