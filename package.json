{
  "name": "cachios",
  "private": true,
  "version": "1.1.0",
  "description": "Simple axios cache wrapper using node-cache",
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "scripts": {
    "lint": "eslint src",
    "build": "node build/build.js",
    "test": "./node_modules/.bin/jest",
    "test-dist": "./node_modules/.bin/jest -c dist.jest.config.js",
    "coveralls": "cat tests/coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "es2015"
          ]
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "tests/coverage",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "^cachios$": "<rootDir>/src"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbinoDrought/cachios.git"
  },
  "keywords": [
    "axios",
    "cache"
  ],
  "author": "AlbinoDrought",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbinoDrought/cachios/issues"
  },
  "homepage": "https://github.com/AlbinoDrought/cachios#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.23.0",
    "node-cache": "^4.1.1",
    "object-hash": "^1.1.8"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^2.1.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.0",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.3.0",
    "express": "^4.16.2",
    "jest": "^22.0.4",
    "mocha": "^5.0.0",
    "moxios": "^0.4.0",
    "ora": "^2.0.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.2"
  }
}
